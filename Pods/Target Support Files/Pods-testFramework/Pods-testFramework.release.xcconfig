CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES = YES
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(SDKROOT)/usr/include/libxml2 $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/KissXML" "${PODS_ROOT}/Headers/Public/Mantle" "${PODS_ROOT}/Headers/Public/MantleXMLAdapter" "${PODS_ROOT}/Headers/Public/libextobjc"
OTHER_CFLAGS = $(inherited) -DDDXML_LIBXML_MODULE_ENABLED=0 $(inherited) -DDDXML_LIBXML_MODULE_ENABLED=1 $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/KissXML" -isystem "${PODS_ROOT}/Headers/Public/Mantle" -isystem "${PODS_ROOT}/Headers/Public/MantleXMLAdapter" -isystem "${PODS_ROOT}/Headers/Public/libextobjc"
OTHER_LDFLAGS = $(inherited) -ObjC -l"KissXML" -l"Mantle" -l"MantleXMLAdapter" -l"libextobjc" -l"xml2" -framework "Foundation"
OTHER_SWIFT_FLAGS = $(inherited) -DDDXML_LIBXML_MODULE_ENABLED
PODS_ROOT = ${SRCROOT}/Pods